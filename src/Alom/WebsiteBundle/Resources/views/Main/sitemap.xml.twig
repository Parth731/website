<?xml version="1.0" encoding="UTF-8"?>
<urlset
      xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
            http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
    <url>
      <loc>http://alexandre-salome.fr/</loc>
      <changefreq>yearly</changefreq>
    </url>
    <url>
      <loc>http://alexandre-salome.fr/cv</loc>
      <changefreq>monthly</changefreq>
    </url>
    <url>
      <loc>http://alexandre-salome.fr/contact</loc>
      <changefreq>monthly</changefreq>
    </url>
    <url>
      <loc>http://alexandre-salome.fr/blog</loc>
      <changefreq>weekly</changefreq>
    </url>

    {% for post in posts %}
        <url>
          <loc>http://alexandre-salome.fr/blog/{{ post.slug }}</loc>
          <changefreq>weekly</changefreq>
        </url>
    {% endfor %}

    <url>
      <loc>http://alexandre-salome.fr/archives/2009-2011/fr</loc>
      <changefreq>never</changefreq>

    </url>
    <url>
      <loc>http://alexandre-salome.fr/archives/2009-2011/fr/contact</loc>
      <changefreq>never</changefreq>
    </url>
</urlset>
